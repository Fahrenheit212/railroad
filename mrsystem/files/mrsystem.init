#!/bin/sh /etc/rc.common
# Copyright (C) 2015 OpenWrt.org

START=99

start() {
	modprobe sun4i_can
	/usr/sbin/ip link set can0 up type can bitrate 250000

	drehscheibe
	sleep 1
	mrcs2eth
#	mrms1
	mrms2
	mrzentrale
}

stop() {
	killall drehscheibe
	killall mrcs2eth
	killall mrms1
	killall mrms2
	killall mrzentrale
}
